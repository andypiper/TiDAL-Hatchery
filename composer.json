{
    "name": "badgeteam/hatchery",
    "description": "Hatchery micropython app repository . .",
    "keywords": ["micropython", "badge", "repository"],
    "authors": [
        {
            "name": "Anne Jan Brouwer",
            "email": "brouwer@annejan.com",
            "homepage": "https://annejan.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hatchery@badge.team",
        "isseus": "https://github.com/BadgeTeam/Hatchery/issues"
    },
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "asbiin/laravel-webauthn": "dev-master",
        "bacon/bacon-qr-code": "^2.0",
        "bepsvpt/secure-headers": "^5.6",
        "czproject/git-php": "^3.18",
        "darkaonline/l5-swagger": "^7",
        "facade/ignition": "^2.4",
        "fruitcake/laravel-cors": "^2.0",
        "intervention/image": "^2.5",
        "laravel/framework": "^7.28",
        "laravel/horizon": "^4.3",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^2.4",
        "laravelcollective/html": "^6.2",
        "livewire/livewire": "^1.3",
        "pragmarx/google2fa-laravel": "^1.4",
        "pragmarx/recovery": "^0.1.0",
        "predis/predis": "^1.1",
        "seld/jsonlint": "^1.8",
        "sentry/sentry-laravel": "^1.9",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-sitemap": "^5.8",
        "symfony/yaml": "^5",
        "verschuur/laravel-robotstxt": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "codacy/coverage": "^1.4",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-laravel5": "^1.1",
        "composer/composer": "^1.10.6",
        "doctrine/dbal": "^2.11",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.4",
        "nunomaduro/larastan": "^0.6.7",
        "pestphp/pest": "^0.3",
        "phan/phan": "^2.7",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
